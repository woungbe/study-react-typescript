{
    "compilerOptions": {
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      // "module": "esnext",
     "module": "ES2020",
     "target": "ES2020",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "noFallthroughCasesInSwitch": true,
      "jsx": "react-jsx",
    },
    "include": [
      "src",
    ]
  }